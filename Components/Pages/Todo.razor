@page "/todo"
@rendermode InteractiveServer

<PageTitle>Todo</PageTitle>

<h3>Todo</h3>

<ul>
    @foreach (var todo in todos)
    {
        <li>todo.Title</li>
    }
</ul>

<input placeholder="Something for me to do"/>
<button> Add Task </button>


@code {
    private List<TodoItem> todos = new();
}
